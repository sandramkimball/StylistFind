{
    "version": 2,
    "name": "StylistFind-FE",
    "builds": [{ 
        "src": "client/package.json", 
        "use": "@now/next", 
        "config": {"distDir": "build"} 
        }],
    "routes": [
        { 
            "src": "/(.*)", 
            "dest": "/client/$1",
            "headers" : {
                "x-request-path": "$1"
            } 
        }
    ]
  }